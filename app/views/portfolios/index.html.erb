<h1>Portfolio Items</h1>

<%= link_to "Create New Item", new_portfolio_url %>


<%= @portfolio.each do |portfolio| %>
<p><%= link_to portfolio.title , portfolio_path(portfolio.id)%></p>
<p><%= portfolio.subtitle %></p>
<p><%= portfolio.body %></p>
<p><%= image_tag portfolio.thumbimage if !portfolio.thumbimage.nil? %></p>
<%= link_to "Edit Item", edit_portfolio_path(portfolio.id) %>
<%= link_to "Delete", portfolio_path(portfolio), method: :delete%>



<% end %>